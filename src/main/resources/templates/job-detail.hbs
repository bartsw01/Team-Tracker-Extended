{{#partial "content"}}
   <p>Team Name:</p>
   <p>{{job.teamName}}</p>
   <h5>Team Description:</h5>
   <p> {{job.teamDescription}}</p>
   <h5>Team Member 1:</h5>
   <p> {{job.teamMember1}}</p>
   <h5>Team Member 2:</h5>
   <p> {{job.teamMember2}}</p>
   <h5>Team Member 3</h5>
   <p> {{job.teamMember3}}</p>
   <h5>Team Member 4</h5>
   <p> {{job.teamMember4}}</p>
   <p>
       {{#if job.addedJob}}
           <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Added
       {{else}}
           <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Pending
       {{/if}}

       <a href="/jobs/{{job.id}}/update">Edit this team</a>
       <a href="/jobs/{{job.id}}/delete">DELETE this team (no undo)</a>
   </p>
{{/partial}}
{{> layout.hbs}}